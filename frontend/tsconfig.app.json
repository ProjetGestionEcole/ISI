{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "outDir": "./out-tsc/app",
    "types": ["node"],
    "moduleResolution": "node",
    "baseUrl": "./",
    "paths": {
      "@primeuix/themes/*": ["node_modules/@primeuix/themes/dist/*"]
    }
  },
  "files": [
    "src/main.ts",
    "src/main.server.ts",
    "server.ts"
  ],
  "include": [
    "src/**/*.d.ts"
  ]
}
